@inproceedings{ ISI:000591176500035,
Author = {Katermina, Veronika V. and Solovyeva, Natalia S.},
Editor = {{Pavlova, A}},
Title = {{GASTRONOMIC VIEW OF THE WORLD IN ENGLISH NEOLOGY: LINGUOCULTURAL ASPECT}},
