@inproceedings{ ISI:000591176500068,
Author = {Sedliarova, Olga M. and Nenasheva, Juliia A. and Solovyeva, Natalia S.},
Editor = {{Pavlova, A}},
Title = {{CULTURAL IDENTITY SHIFT IN THE INTERNET MEME LOANS}},
