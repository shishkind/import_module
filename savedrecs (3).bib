@article{ ISI:000537214800001,
Author = {Wang, Lin and Liu, Juan and Kong, Charlie and Pesin, Alexander and
   Zhilyaev, Alexander P. and Yu, Hailiang},
Title = {{Sandwich-Like Cu/Al/Cu Composites Fabricated by Cryorolling}},
Journal = {{ADVANCED ENGINEERING MATERIALS}},
Year = {{2020}},
Volume = {{22}},
Number = {{10}},
Month = {{OCT}},
Abstract = {{Herein, sandwich-like Cu/Al/Cu composites are fabricated by hot rolling
   (300 degrees C), cold rolling (25 degrees C), cryorolling (-100 and -190
   degrees C), respectively. Tensile tests are conducted and the fracture
   surfaces of both the Al and Cu sides are examined using scanning
   electron microscopy (SEM) for fractography and SEM-based
   energy-dispersive analysis. The microstructure of the composites is
   evaluated by optical microscopy and SEM. Results reveal that the highest
   ultimate tensile strength and the best ductility of the composites have
   been achieved using cryorolling of the rolling temperature -100 degrees
   C. Finally, discussion on grain size, bonding quality, and the thickness
   of the intermetallic layer between Cu and Al layers on the mechanical
   properties of the sandwich-like Cu/Al/Cu composites is focused.}},
