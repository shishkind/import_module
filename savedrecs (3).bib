Author = {Gasiyarov, V. R. and Radionov, A. A. and Karandaev, A. S. and Khramshin,
   V. R. and Loginov, B. M.},
Book-Group-Author = {{IEEE}},
Title = {{Improving the Algorithm of Automated Gage Control during Shaped Feed
   Rolling on a Plate Mill}},
Booktitle = {{PROCEEDINGS OF 2020 IEEE 11TH INTERNATIONAL CONFERENCE ON MECHANICAL AND
   INTELLIGENT MANUFACTURING TECHNOLOGIES (ICMIMT 2020)}},
Year = {{2020}},
