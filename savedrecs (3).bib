@article{ ISI:000576646300018,
Author = {Soldatchenko, Aleksandr L. and Butova, V, Anna and Dubskikh, I, Angelina
   and Kisel, V, Olesya},
Title = {{MODERN APPROACHES TO THE PROCESS OF SOCIALIZATION OF UNIVERSITY STUDENTS}},
Journal = {{REVISTA INCLUSIONES}},
