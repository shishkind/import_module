Title = {{Failure Model for Gear Couplings Under the Criterion of Working Surface
   Endurance}},
Booktitle = {{PROCEEDINGS OF THE 5TH INTERNATIONAL CONFERENCE ON INDUSTRIAL
   ENGINEERING, ICIE 2019, VOL I}},
